{
  byte b1, b2;
  int length=0;
  int end=data.length();
  while (end > 0) {
    if (!ignore(data.charAt(end - 1))) {
      break;
    }
    end--;
  }
  int i=0;
  while (i < end) {
    while (i < end && ignore(data.charAt(i))) {
      i++;
    }
    b1=decodingTable[data.charAt(i++)];
    while (i < end && ignore(data.charAt(i))) {
      i++;
    }
    b2=decodingTable[data.charAt(i++)];
    out.write((b1 << 4) | b2);
    length++;
  }
  return length;
}
