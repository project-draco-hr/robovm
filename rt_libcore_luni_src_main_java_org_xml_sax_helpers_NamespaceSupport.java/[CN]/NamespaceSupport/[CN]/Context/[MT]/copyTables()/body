{
  if (prefixTable != null) {
    prefixTable=(Hashtable)prefixTable.clone();
  }
 else {
    prefixTable=new Hashtable();
  }
  if (uriTable != null) {
    uriTable=(Hashtable)uriTable.clone();
  }
 else {
    uriTable=new Hashtable();
  }
  elementNameTable=new Hashtable();
  attributeNameTable=new Hashtable();
  declSeen=true;
}
