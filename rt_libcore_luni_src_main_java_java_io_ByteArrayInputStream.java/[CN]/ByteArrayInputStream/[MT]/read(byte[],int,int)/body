{
  Arrays.checkOffsetAndCount(buffer.length,byteOffset,byteCount);
  if (this.pos >= this.count) {
    return -1;
  }
  if (byteCount == 0) {
    return 0;
  }
  int copylen=this.count - pos < byteCount ? this.count - pos : byteCount;
  System.arraycopy(this.buf,pos,buffer,byteOffset,copylen);
  pos+=copylen;
  return copylen;
}
