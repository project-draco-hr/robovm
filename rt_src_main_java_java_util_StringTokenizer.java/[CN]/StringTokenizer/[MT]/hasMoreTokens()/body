{
  if (delimiters == null) {
    throw new NullPointerException();
  }
  int length=string.length();
  if (position < length) {
    if (returnDelimiters)     return true;
    for (int i=position; i < length; i++)     if (delimiters.indexOf(string.charAt(i),0) == -1)     return true;
  }
  return false;
}
