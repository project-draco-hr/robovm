{
  if (obj == null || obj instanceof X509Name) {
    return (X509Name)obj;
  }
 else   if (obj instanceof X500Name) {
    return new X509Name(ASN1Sequence.getInstance(((X500Name)obj).getDERObject()));
  }
 else   if (obj != null) {
    return new X509Name(ASN1Sequence.getInstance(obj));
  }
  return null;
}
