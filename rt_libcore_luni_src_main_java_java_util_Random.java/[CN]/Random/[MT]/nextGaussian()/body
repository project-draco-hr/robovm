{
  if (haveNextNextGaussian) {
    haveNextNextGaussian=false;
    return nextNextGaussian;
  }
  double v1, v2, s;
  do {
    v1=2 * nextDouble() - 1;
    v2=2 * nextDouble() - 1;
    s=v1 * v1 + v2 * v2;
  }
 while (s >= 1);
  double norm=Math.sqrt(-2 * Math.log(s) / s);
  nextNextGaussian=v2 * norm;
  haveNextNextGaussian=true;
  return v1 * norm;
}
