{
  final Object[] items=this.items;
  if (i == takeIndex) {
    items[takeIndex]=null;
    takeIndex=inc(takeIndex);
  }
 else {
    for (; ; ) {
      int nexti=inc(i);
      if (nexti != putIndex) {
        items[i]=items[nexti];
        i=nexti;
      }
 else {
        items[i]=null;
        putIndex=i;
        break;
      }
    }
  }
  --count;
  notFull.signal();
}
