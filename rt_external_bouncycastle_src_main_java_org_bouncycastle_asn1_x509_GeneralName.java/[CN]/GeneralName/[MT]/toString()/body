{
  StringBuffer buf=new StringBuffer();
  buf.append(tag);
  buf.append(": ");
switch (tag) {
case rfc822Name:
case dNSName:
case uniformResourceIdentifier:
    buf.append(DERIA5String.getInstance(obj).getString());
  break;
case directoryName:
buf.append(X509Name.getInstance(obj).toString());
break;
default :
buf.append(obj.toString());
}
return buf.toString();
}
