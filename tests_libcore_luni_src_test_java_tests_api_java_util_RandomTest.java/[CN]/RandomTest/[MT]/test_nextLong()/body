{
  long lastNum=r.nextLong();
  long nextNum;
  boolean someDifferent=false;
  for (int counter=0; counter < 100; counter++) {
    nextNum=r.nextLong();
    if (nextNum != lastNum)     someDifferent=true;
    lastNum=nextNum;
  }
  assertTrue("Calling nextLong 100 times resulted in same number",someDifferent);
}
