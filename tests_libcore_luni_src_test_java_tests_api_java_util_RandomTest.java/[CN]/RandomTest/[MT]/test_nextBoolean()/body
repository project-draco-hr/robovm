{
  boolean falseAppeared=false, trueAppeared=false;
  for (int counter=0; counter < 100; counter++)   if (r.nextBoolean())   trueAppeared=true;
 else   falseAppeared=true;
  assertTrue("Calling nextBoolean() 100 times resulted in all trues",falseAppeared);
  assertTrue("Calling nextBoolean() 100 times resulted in all falses",trueAppeared);
}
